{
  "@context": {
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "ex": "https://example.org/ns#",
    "id": "@id",
    "type": "@type",
    "ex:properties": {
        "@container": "@list"
    }
  },
  "@graph": [
    {
      "id": "ex:Game",
      "type": "owl:Class",
      "ex:properties": [
        { "id": "ex:title" },
        { "id": "ex:slug" }
      ]
    },
    {
      "id": "ex:properties",
      "type": "rdf:Property"
    },
    {
      "id": "ex:slug",
      "type": [ "owl:DataProperty", "owl:FunctionalProperty" ]
    },
    {
      "id": "ex:title",
      "type": "owl:DataProperty"
    }
  ]
}
