		<exists/manifest.ttl>
		<functions/manifest.ttl>
		<grouping/manifest.ttl>
		<json-res/manifest.ttl>
		<move/manifest.ttl>
		<negation/manifest.ttl>
		<project-expression/manifest.ttl>
		<property-path/manifest.ttl>
		<service/manifest.ttl>
		<subquery/manifest.ttl>
