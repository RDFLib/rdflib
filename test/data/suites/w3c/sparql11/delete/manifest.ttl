    dawgt:approvedBy <http://www.w3.org/2009/sparql/meeting/2012-02-07#resolution_3> ;
    mf:action [ ut:request <delete-01.ru> ; 
                ut:data <delete-pre-01.ttl>
              ] ;
    mf:result [ ut:data <delete-post-01s.ttl>
              ] .

