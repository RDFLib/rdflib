              ] ;
    mf:result [
    			ut:data <empty.ttl> ;
                ut:graphData [ ut:graph <empty.ttl> ;
                               rdfs:label "http://example.org/g1" ] ;
                ut:graphData [ ut:graph <empty.ttl> ;
                               rdfs:label "http://example.org/g2" ] ;
              ] ;
    .

