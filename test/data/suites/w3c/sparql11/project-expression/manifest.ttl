    . 
    
:projexp05 rdf:type mf:QueryEvaluationTest ;
    mf:name    "Expression may return no value" ;
    dawgt:approval dawgt:Approved;
    dawgt:approvedBy <http://www.w3.org/2009/sparql/meeting/2010-09-07#resolution_3> ;
