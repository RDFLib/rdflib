<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:eg="http://example.org/">

 <!-- In this example the rdf:nodeID is redundant. -->
 <rdf:Description rdf:nodeID="a" eg:property1="value" />

</rdf:RDF>
