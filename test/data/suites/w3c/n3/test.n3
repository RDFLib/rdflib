####
# Notation-3 Test Vocabulary

@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:   <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix rdft: <http://www.w3.org/ns/rdftest#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix test: <#> .

test:TestN3Reason a rdfs:Class;
   rdfs:label "N3 Positive Reasoning"@en;
   rdfs:comment """
     Tests reasoning over N3 rules to produce an output dataset described by results.
    """"@en;
   rdfs:subClassOf rdft:TestEval .

test:TestN3Eval a rdfs:Class;
   rdfs:label "N3 Positive Evaluation"@en;
   rdfs:comment """
     Tests transformation of N3 into an isomorphic representation.
    """"@en;
   rdfs:subClassOf rdft:TestEval .

test:TestN3PositiveSyntax a rdfs:Class;
   rdfs:label "N3 Positive Syntax"@en;
   rdfs:comment "Tests parsing N3 without error."@en;
   rdfs:subClassOf rdft:TestSyntax .

test:TestN3NegativeSyntax a rdfs:Class;
   rdfs:label "N3 Negative Syntax"@en;
   rdfs:comment "Tests parsing N3 with expected errors"@en;
   rdfs:subClassOf rdft:TestSyntax .

test:Option a rdfs:Class;
  rdfs:label "Processor Options" ;
  rdfs:comment "Options passed to the test runner to affect the reasoner." .

test:conclusions a rdf:Property;
  rdfs:comment "Replace store with conclusions from rules in store";
  rdfs:domain test:Option;
  rdfs:range rdfs:Resource .

test:data a rdf:Property;
  rdfs:comment "Remove all except plain RDF triples (formulae, forAll, etc)";
  rdfs:domain test:Option;
  rdfs:range xsd:boolean .

test:filter a rdf:Property;
  rdfs:comment "Read rules from resource, apply to store, REPLACING store with conclusions";
  rdfs:domain test:Option;
  rdfs:range rdfs:Resource .

test:strings a rdf:Property;
  rdfs:comment "Output concatenated strings generated by log:outputString";
  rdfs:domain test:Option;
  rdfs:range xsd:boolean .

test:options a rdf:Property;
  rdfs:domain test:CwmTest;
  rdfs:range test:Option;
  rdfs:comment "Options for running tests" .

test:rules a rdf:Property;
  rdfs:comment "Apply rules in store to store, adding conclusions to store";
  rdfs:domain test:Option;
  rdfs:domain xsd:boolean .

test:think a rdf:Property;
  rdfs:comment "as test:rules but continue until no more rule matches (or forever!)";
  rdfs:domain test:Option;
  rdfs:domain xsd:boolean .
